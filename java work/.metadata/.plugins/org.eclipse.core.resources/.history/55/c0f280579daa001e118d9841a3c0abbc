package ex04;

public class Ex01 {
	public static void main(String[] args) {
		//형변환 연산자
		int a = 10;
		double b = 20;
		System.out.println("a = "+ a);
		System.out.println("b = "+ b);
		
		int c = (double)b;
		double d = (int)a;
		System.out.println("c = "+c);
		System.out.println("d = "+d);
		
	}

}
